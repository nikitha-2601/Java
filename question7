public class Main {
  public static void main(String[] args) {
    double myDouble = 7.22;

  
    int myInt = (int) myDouble; // Fractional part is truncated

    
    System.out.println("Original double value: " + myDouble);
    System.out.println("Double cast to int: " + myInt);
    int anotherInt = 42;
    double convertedDouble = (double) anotherInt;

    // Display the result of the casting
    System.out.println("Original int value: " + anotherInt);
    System.out.println("Int cast to double: " + convertedDouble);
  }
}
